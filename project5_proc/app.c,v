#include<stdio.h>
#include<fcntl.h>
int main(int argc,char *argv[])
{
	int fd;
	fd=open(argv[1],O_RDONLY);
	if(fd<0)
		printf("File not opened");
	else
		printf("File opened Successfully");
	close(fd);
	return 0;
}

